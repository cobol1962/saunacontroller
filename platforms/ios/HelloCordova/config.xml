<?xml version='1.0' encoding='utf-8'?>
<widget id="com.saunacontrol.saunacontroller" version="1.0.0" versionCode="1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="WifiInfo">
        <param name="ios-package" value="WifiInfo" />
    </feature>
    <feature name="Battery">
        <param name="ios-package" value="CDVBattery" />
    </feature>
    <feature name="Camera">
        <param name="ios-package" value="CDVCamera" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <feature name="Globalization">
        <param name="ios-package" value="CDVGlobalization" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="Capture">
        <param name="ios-package" value="CDVCapture" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="SocketsForCordova">
        <param name="ios-package" value="SocketPlugin" />
    </feature>
    <feature name="Vibration">
        <param name="ios-package" value="CDVVibration" />
    </feature>
    <feature name="networkinterface">
        <param name="ios-package" onload="true" value="CDVNetworkInterface" />
    </feature>
    <feature name="WifiWizard">
        <param name="ios-package" value="NXWWifiWizard" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <name>SENSUM CONTROLLER</name>
    <description>
        Keep your szauna under control from anywhere.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Sauna Controller team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <splash platform="android" qualifier="port-ldpi" src="res/screen/android/SaunaIcon2.jpg" />
    <splash platform="android" qualifier="port-mdpi" src="res/screen/android/Saunaicon2.jpg" />
    <splash platform="android" qualifier="port-hdpi" src="res/screen/android/Saunaicon2.jpg" />
    <splash platform="android" qualifier="port-xhdpi" src="res/screen/android/Saunaicon2.jpg" />
    <splash platform="android" qualifier="land-ldpi" src="res/screen/android/Saunaicon2.jpg" />
    <splash platform="android" qualifier="land-mdpi" src="res/screen/android/Saunaicon2.jpg" />
    <splash platform="android" qualifier="land-hdpi" src="res/screen/android/Saunaicon2.jpg" />
    <splash platform="android" qualifier="land-xhdpi" src="res/screen/android/Saunaicon2.jpg" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <splash height="480" src="res/screen/ios/Saunaicon2.jpg" width="320" />
    <splash height="960" src="res/screen/ios/Saunaicon2.jpg" width="640" />
    <splash height="1024" src="res/screen/ios/Saunaicon2.jpg" width="768" />
    <splash height="2048" src="res/screen/ios/Saunaicon2.jpg" width="1536" />
    <splash height="768" src="res/screen/ios/Saunaicon2.jpg" width="1024" />
    <splash height="1536" src="res/screen/ios/Saunaicon2.jpg" width="2048" />
    <splash height="1136" src="res/screen/ios/Saunaicon2.jpg" width="640" />
    <splash height="1334" src="res/screen/ios/Saunaicon2.jpg" width="750" />
    <splash height="2208" src="res/screen/ios/Saunaicon2.jpg" width="1242" />
    <splash height="1242" src="res/screen/ios/Saunaicon2.jpg" width="2208" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="true" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="CameraUsesGeolocation" value="false" />
    <preference name="android-minSdkVersion" value="19" />
</widget>
